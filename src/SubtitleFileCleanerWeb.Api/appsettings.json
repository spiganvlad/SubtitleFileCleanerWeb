{
  "ConnectionStrings": {
    "SQLite": "Data Source=app.sqlite;Cache=Shared"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cors": {
    "DefaultPolicy": {
      "Origins": [ "*" ],
      "Headers": [ "*" ],
      "Methods": [ "*" ]
    }
  },
  "FormFile": {
    "MaxFileLength": "1000000"
  },
  "AllowedHosts": "*"
}
